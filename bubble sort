#include <iostream>
#include<bits/stdc++.h>
using namespace std;
int main()
{
    vector<int>a={3,4,1,0,8,-9};
    for(int i=0;i<a.size();i++){
        for(int j=0;j<a.size()-i-1;j++){
            if(a[j]>a[j+1]){
                swap(a[j],a[j+1]);
            }
        }
    }
    for(int i=0;i<a.size();i++){
        cout<<a[i]<<" ";
    }
}
